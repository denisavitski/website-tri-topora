---
import Breadcrumbs from '@blocks/Breadcrumbs.astro'
import Layout from '@layout/Layout.astro'
import Layouts from '@blocks/Layouts.astro'
import Interiors from '@blocks/Interiors.astro'
import Feedback from '@blocks/Feedback.astro'
import ProjectIntro from '@blocks/ProjectIntro.astro'
---

<Layout
  seo={{
    title: `Три топора: Название проекта`,
    description:
      'Строительство из дерева, стекла и камня сравнимо с исскуством',
    cover: '/images/seo/home.jpg',
  }}
>
  <Breadcrumbs
    links={[
      {
        name: 'Главная',
        href: '/',
      },
      {
        name: 'Проекты',
        href: '/projects',
      },
      {
        name: `Название проекта`,
        href: `/projects/project`,
      },
    ]}
  />
  <ProjectIntro />
  <Interiors />
  <Layouts />
  <Feedback />
</Layout>
