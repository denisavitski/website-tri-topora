{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": false,
  "printWidth": 999999,
  "arrowParens": "always",
  "bracketSpacing": true,
  "singleAttributePerLine": true,
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
